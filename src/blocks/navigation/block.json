{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "premium/navigation",
    "title": "Mega Menu",
    "category": "premium-blocks",
    "description": "A collection of blocks that allow visitors to get around your site.",
    "keywords": ["menu", "navigation", "links"],
    "textdomain": "default",
    "attributes": {
        "blockId": {
            "type": "string"
        },
        "ref": {
            "type": "number"
        },
        "overlayMenu": {
            "type": "string",
            "default": "mobile"
        },
        "hasIcon": {
            "type": "boolean",
            "default": true
        },
        "overlayColors": {
            "type": "object",
            "default": {
                "link": "",
                "linkHover": "",
                "background": "",
                "icon": "",
                "iconHover": ""
            }
        },
        "menuColors": {
            "type": "object",
            "default": {
                "link": "",
                "linkHover": "",
                "background": ""
            }
        },
        "submenuColors": {
            "type": "object",
            "default": {
                "link": "",
                "linkHover": "",
                "background": ""
            }
        },
        "menuBorder": {
            "type": "object",
            "default": {
                "borderType": "none",
                "borderColor": "",
                "borderWidth": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                },
                "borderRadius": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                }
            }
        },
        "overlayMenuBorder": {
            "type": "object",
            "default": {
                "borderType": "none",
                "borderColor": "",
                "borderWidth": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                },
                "borderRadius": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                }
            }
        },
        "submenuBorder": {
            "type": "object",
            "default": {
                "borderType": "none",
                "borderColor": "",
                "borderWidth": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                },
                "borderRadius": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                }
            }
        },
        "spacing": {
            "type": "object",
            "default": {
                "padding": null,
                "margin": null,
                "itemPadding": null
            }
        },
        "typography": {
            "type": "object",
            "default": {
                "fontWeight": "400",
                "fontStyle": "",
                "letterSpacing": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "fontFamily": "Default",
                "lineHeight": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "textDecoration": "",
                "textTransform": "",
                "fontSize": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                }
            }
        },
        "submenuTypography": {
            "type": "object",
            "default": {
                "fontWeight": "400",
                "fontStyle": "",
                "letterSpacing": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "fontFamily": "Default",
                "lineHeight": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "textDecoration": "",
                "textTransform": "",
                "fontSize": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                }
            }
        },
        "showSubmenuIcon": {
            "type": "boolean",
            "default": true
        },
        "openSubmenusOnClick": {
            "type": "boolean",
            "default": false
        },
        "__unstableLocation": {
            "type": "string"
        },
        "menuStyle": {
            "type": "string"
        },
        "dropdownReveal": {
            "type": "string"
        },
        "submenuWidth": {
            "type": "number"
        },
        "submenuShadow": {
            "type": "boolean",
            "default": false
        },
        "mobileBreakPoint": {
            "type": "number"
        },
        "overlayMenuStyle": {
            "type": "string",
            "default": "full"
        },
        "overlayMenuWidth": {
            "type": "number",
            "default": 600
        },
        "hideDesktop": {
            "type": "boolean"
        },
        "hideTablet": {
            "type": "boolean"
        },
        "hideMobile": {
            "type": "boolean"
        }
    },
    "usesContext": ["navigationArea"],
    "providesContext": {
        "textColor": "textColor",
        "customTextColor": "customTextColor",
        "backgroundColor": "backgroundColor",
        "customBackgroundColor": "customBackgroundColor",
        "showSubmenuIcon": "showSubmenuIcon",
        "openSubmenusOnClick": "openSubmenusOnClick",
        "menuColors": "menuColors",
        "submenuColors": "submenuColors",
        "style": "style",
        "orientation": "orientation",
        "submenuWidth": "submenuWidth",
        "dropdownReveal": "dropdownReveal",
        "submenuTypography": "submenuTypography",
        "menuTypography": "typography",
        "overlayMenu": "overlayMenu",
        "submenuBorder": "submenuBorder"
    },
    "supports": {
        "align": ["wide", "full"],
        "anchor": true,
        "html": false,
        "inserter": true,
        "__experimentalLayout": {
            "allowSwitching": false,
            "allowInheriting": false,
            "default": {
                "type": "flex"
            }
        }
    },
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css"
}
