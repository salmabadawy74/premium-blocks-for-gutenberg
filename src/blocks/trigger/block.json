{
    "apiVersion": 2,
    "version": "0.1.0",
    "name": "premium/trigger",
    "title": "Trigger Icon",
    "icon": "menu-alt",
    "category": "premium-blocks",
	"description": "Example static block scaffolded with Create Block tool.",
	"keywords": ["trigger", "overlay-canvas", "canvas"],
	"supports": {
		"html": false
	},
	"editorScript": "file:./build/block.js",
	"script": "gpb-trigger-block-script",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-block.css",
	"attributes": {
		"text": {
			"type": "string"
		},
		"block_id": {
			"type": "string"
		},
		"triggerLabel": {
			"type": "string"
		},
		"iconSize": {
			"type": "number"
		},
		"iconAlignment": {
			"type": "string",
			"default": "left"
		},
		"hOffset": {
			"type": "number",
			"default": "10"	
		},
		"vOffset": {
			"type": "number",
			"default": "10"
		},
		"floatPosition": {
			"type": "string",
			"default": "topright"
		},
		"triggerStyles": {
            "type": "object",
            "default": {
                "iconColor": "",
                "iconBgColor": "#e8e8e8",
                "iconHoverColor": "",
				"labelHoverColor": "",
                "iconHoverBgColor": "",
				"labelPosition": "left",
				"style": "simple",
				"borderType": "solid",
				"borderColor": "#e8e8e8",
				"borderHoverColor": "",
				"borderRadius": ""
            }
        },
		"canvasStyles": {
            "type": "object",
            "default": {
				"overlayBgColor": "",
                "canvasBgColor": "#fff",
				"layout": "right",
				"width": ""
            }
        },
		"spacing": {
            "type": "object",
            "default": {
                "padding": null
            }
        },
		"triggerBorderTop": {
			"type": "number",
			"default": 1
		},
		"triggerBorderRight": {
			"type": "number",
			"default": 1
		},
		"triggerBorderBottom": {
			"type": "number",
			"default": 1
		},
		"triggerBorderLeft": {
			"type": "number",
			"default": 1
		}
	}

}