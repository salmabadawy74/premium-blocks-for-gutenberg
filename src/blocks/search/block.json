{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "premium/search",
    "title": "Search",
    "category": "premium-blocks",
    "description": "Allow your website visitors to easily find what they are looking for using Premium Search Block.",
    "keywords": [
        "find"
    ],
    "textdomain": "default",
    "attributes": {
        "blockId": {
            "type": "string"
        },
        "label": {
            "type": "string",
            "__experimentalRole": "content"
        },
        "border": {
            "type": "object",
            "default": {
                "borderType": "solid",
                "borderColor": "",
                "borderWidth": {
                    "Desktop": {
                        "top": "1",
                        "right": "1",
                        "bottom": "1",
                        "left": "1"
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                },
                "borderRadius": {
                    "Desktop": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Tablet": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    },
                    "Mobile": {
                        "top": "",
                        "right": "",
                        "bottom": "",
                        "left": ""
                    }
                }
            }
        },
        "colors": {
            "type": "object",
            "default": {
                "background": "",
                "text": "",
                "btnBackground": "var(--pbBackgroundColor, #f2f2f2)",
                "btnText": "",
                "btnHoverBackground": "",
                "btnHoverText": "",
                "label": "",
                "modal": "",
                "dropdown": "",
                "link": "",
                "linkHover": ""
            }
        },
        "position": {
            "type": "string",
            "default": "static"
        },
        "floatPosition": {
            "type": "string",
            "default": "top-right"
        },
        "floatValues": {
            "type": "object",
            "default": {
                "top": 10,
                "right": 10,
                "bottom": 10,
                "left": 10
            }
        },
        "formStyle": {
            "type": "string",
            "default": "default"
        },
        "showLabel": {
            "type": "boolean",
            "default": true
        },
        "ajaxSearch": {
            "type": "boolean",
            "default": true
        },
        "placeholder": {
            "type": "string",
            "default": "",
            "__experimentalRole": "content"
        },
        "buttonText": {
            "type": "string",
            "__experimentalRole": "content"
        },
        "buttonPosition": {
            "type": "string",
            "default": "button-outside"
        },
        "buttonUseIcon": {
            "type": "boolean",
            "default": false
        },
        "typography": {
            "type": "object",
            "default": {
                "fontWeight": "400",
                "fontStyle": "",
                "letterSpacing": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "fontFamily": "Default",
                "lineHeight": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "textDecoration": "",
                "textTransform": "",
                "fontSize": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                }
            }
        },
        "buttonTypography": {
            "type": "object",
            "default": {
                "fontWeight": "400",
                "fontStyle": "",
                "letterSpacing": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "fontFamily": "Default",
                "lineHeight": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                },
                "textDecoration": "",
                "textTransform": "",
                "fontSize": {
                    "Desktop": "",
                    "Tablet": "",
                    "Mobile": "",
                    "unit": "px"
                }
            }
        },
        "spacing": {
            "type": "object",
            "default": {
                "padding": null,
                "buttonPadding": null
            }
        },
        "hideDesktop": {
            "type": "boolean"
        },
        "hideTablet": {
            "type": "boolean"
        },
        "hideMobile": {
            "type": "boolean"
        }
    },
    "supports": {
        "align": [
            "left",
            "center",
            "right"
        ],
        "html": false
    },
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css"
}